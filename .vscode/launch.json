  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Tonio consume",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "args": [
        "--host",
        "localhost:5672",
        "-u",
        "tonio",
        "-p",
        "tonio",
        "-e",
        "testing.dev",
        "-t",
        "topic",
        "consume",
        "testing.messages"
      ]
    }
  ]
}
